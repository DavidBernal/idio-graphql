    "apollo-server-express": "^2.9.16",
    "core-js": "^3.6.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-subscriptions": "^0.1.0",
    "graphql-toolkit": "^0.7.4",
